#include<stdio.h>
int main(){
int n;
printf("Enter Year:");
scanf("%d",&n);
if(n%4==0){
printf("It is leap year");
} else{
printf("It is not a leap year");
}
return 0;
}
/*
Enter Year:2024
It is leap year

Enter Year:2022
It is not a leap year
*/
